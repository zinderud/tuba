<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:zinderud="clr-namespace:Zinderud.Windows.Controls;assembly=Zinderud.Windows.Controls" xmlns:zinderudPrimitives="clr-namespace:Zinderud.Windows.Controls.Primitives;assembly=Zinderud.Windows.Controls" xmlns:dragDrop="clr-namespace:Zinderud.Windows.Controls.DragDrop;assembly=Zinderud.Windows.Controls" xmlns:zinderudDragDrop="clr-namespace:Zinderud.Windows.DragDrop;assembly=Zinderud.Windows.Controls" xmlns:animation="clr-namespace:Zinderud.Windows.Controls.Animation;assembly=Zinderud.Windows.Controls" xmlns:zinderudChromes="clr-namespace:Zinderud.Windows.Controls.Chromes;assembly=Zinderud.Windows.Controls" xmlns:sys="clr-namespace:System;assembly=mscorlib" xmlns:zinderudTransitions="clr-namespace:Zinderud.Windows.Controls.TransitionEffects;assembly=Zinderud.Windows.Controls" xmlns:zinderudTransitonControl="clr-namespace:Zinderud.Windows.Controls.TransitionControl;assembly=Zinderud.Windows.Controls" xmlns:filteringEditors="clr-namespace:Zinderud.Windows.Controls.Filtering.Editors;assembly=Zinderud.Windows.Controls" xmlns:zinderudShapes="clr-namespace:Zinderud.Windows.Shapes;assembly=Zinderud.Windows.Controls" xmlns:transitionControl="clr-namespace:Zinderud.Windows.Controls.TransitionControl;assembly=Zinderud.Windows.Controls" xmlns:touch="clr-namespace:Zinderud.Windows.Input.Touch;assembly=Zinderud.Windows.Controls" xmlns:legend="clr-namespace:Zinderud.Windows.Controls.Legend;assembly=Zinderud.Windows.Controls">
  <ResourceDictionary.MergedDictionaries>
    <zinderud:Windows8TouchResourceDictionary />
  </ResourceDictionary.MergedDictionaries>
  <!-- Brushes-->
  <zinderud:Windows8TouchColors x:Key="Windows8TouchColors" />
  <ControlTemplate x:Key="ScrollArrow_Right" TargetType="ContentControl">
    <Grid>
      <Path x:Name="BackgroundIcon" Data="M0,0 L0,2 1,1 Z" Stretch="Fill" Width="4" Height="7" Fill="{TemplateBinding Background}" Margin="0 2 0 0" />
      <Path x:Name="ForegroundIcon" Data="M0,0 L0,2 1,1 Z" Stretch="Fill" Width="4" Height="7" Fill="{TemplateBinding Foreground}" Margin="0 1 0 1" />
    </Grid>
  </ControlTemplate>
  <SolidColorBrush x:Key="ButtonIconForeground_Normal" Color="#FF282828" />
  <SolidColorBrush x:Key="ButtonIconBackground_Normal" Color="Transparent" />
  <SolidColorBrush x:Key="ButtonIconForeground_MouseOver" Color="#FF282828" />
  <SolidColorBrush x:Key="ButtonIconBackground_MouseOver" Color="Transparent" />
  <SolidColorBrush x:Key="ButtonIconForeground_Pressed" Color="#FF282828" />
  <SolidColorBrush x:Key="ButtonIconBackground_Pressed" Color="Transparent" />
  <SolidColorBrush x:Key="ButtonIconForeground_Disabled" Color="#FF282828" />
  <SolidColorBrush x:Key="ButtonIconBackground_Disabled" Color="Transparent" />
  <ControlTemplate x:Key="ScrollArrow_Left" TargetType="ContentControl">
    <Grid>
      <Path x:Name="BackgroundIcon" Data="M1,0 L0,1 1,2 Z" Stretch="Fill" Width="4" Height="7" Fill="{TemplateBinding Background}" Margin="0 2 0 0" />
      <Path x:Name="ForegroundIcon" Data="M1,0 L0,1 1,2 Z" Stretch="Fill" Width="4" Height="7" Fill="{TemplateBinding Foreground}" Margin="0 1 0 1" />
    </Grid>
  </ControlTemplate>
  <ControlTemplate x:Key="ScrollArrow_Down" TargetType="ContentControl">
    <Grid>
      <Path x:Name="BackgroundIcon" Data="M0,0 L2,0 1,1 Z" Stretch="Fill" Width="7" Height="4" Fill="{TemplateBinding Background}" Margin="0 2 0 0" />
      <Path x:Name="ForegroundIcon" Data="M0,0 L2,0 1,1 Z" Stretch="Fill" Width="7" Height="4" Fill="{TemplateBinding Foreground}" Margin="0 1 0 1" />
    </Grid>
  </ControlTemplate>
  <ControlTemplate x:Key="ScrollArrow_Up" TargetType="ContentControl">
    <Grid>
      <Path x:Name="BackgroundIcon" Data="M0,1 L1,0 2,1 Z" Stretch="Fill" Width="7" Height="4" Fill="{TemplateBinding Background}" Margin="0 2 0 0" />
      <Path x:Name="ForegroundIcon" Data="M0,1 L1,0 2,1 Z" Stretch="Fill" Width="7" Height="4" Fill="{TemplateBinding Foreground}" Margin="0 1 0 1" />
    </Grid>
  </ControlTemplate>
  <ControlTemplate x:Key="ScrollBarThumbGripHorizontal" TargetType="ContentControl">
    <Grid>
      <Rectangle Fill="{TemplateBinding Foreground}" Width="6" Height="9" />
      <Path Stroke="{TemplateBinding Background}" Data="M0.5,0 L0.5,9 M2.5,0 L2.5,9 M4.5,0 L4.5,9" Width="6" Height="9" />
    </Grid>
  </ControlTemplate>
  <SolidColorBrush x:Key="ScrollBarThumbGripFill_Normal" Color="#FF898C95" />
  <SolidColorBrush x:Key="ScrollBarThumbGripStroke_Normal" Color="#FFFFFFFF" />
  <SolidColorBrush x:Key="ScrollBarThumbGripFill_MouseOver" Color="#FF3C7FB1" />
  <SolidColorBrush x:Key="ScrollBarThumbGripStroke_MouseOver" Color="#FFFFFFFF" />
  <SolidColorBrush x:Key="ScrollBarThumbGripFill_Disabled" Color="#FF898C95" />
  <SolidColorBrush x:Key="ScrollBarThumbGripStroke_Disabled" Color="#FFFFFFFF" />
  <ControlTemplate x:Key="ScrollBarThumbGripVertical" TargetType="ContentControl">
    <Grid>
      <Rectangle Fill="{TemplateBinding Foreground}" Width="9" Height="6" />
      <Path Stroke="{TemplateBinding Background}" Data="M0,0.5 L9,0.5 M0,2.5 L9,2.5 M0,4.5 L9,4.5" Width="9" Height="6" />
    </Grid>
  </ControlTemplate>
  <SolidColorBrush x:Key="ControlOuterBorder_Normal" Color="#FF898C95" />
  <Color x:Key="ControlBackground_Normal_Stop0">#FFF2F2F2</Color>
  <SolidColorBrush x:Key="ControlInnerBorder_Normal" Color="#FFFCFCFC" />
  <Color x:Key="ControlBackground_Normal_Stop3">#FFD0D0D0</Color>
  <LinearGradientBrush x:Key="RangeControlBackground_Normal" StartPoint="0.5,0" EndPoint="0.5,1">
    <GradientStop Color="#FFDAE1E4" Offset="0.033" />
    <GradientStop Color="#FFFAFAFA" Offset="0.586" />
  </LinearGradientBrush>
  <LinearGradientBrush x:Key="RangeControlBackgroundVertical_Normal" EndPoint="1,0.5" StartPoint="0,0.5">
    <GradientStop Color="#FFDAE1E4" Offset="0.033" />
    <GradientStop Color="#FFFAFAFA" Offset="0.586" />
  </LinearGradientBrush>
  <LinearGradientBrush x:Key="DragDrop_VisualCueBackground" EndPoint="0.5,0" StartPoint="0.5,1">
    <GradientStop Color="#FFF1F8FD" Offset="0" />
    <GradientStop Color="#FFD5EFFC" Offset="1" />
  </LinearGradientBrush>
  <SolidColorBrush x:Key="DragDrop_VisualCueBorder" Color="#FFA6D9F4" />
  <SolidColorBrush x:Key="MainBrush" Color="{Binding Path=Palette.MainColor, Source={StaticResource Windows8TouchColors}}" zinderud:Windows8TouchPalette.IsFreezable="True" />
  <SolidColorBrush x:Key="MediumBrush" Color="{Binding Path=Palette.MediumColor, Source={StaticResource Windows8TouchColors}}" zinderud:Windows8TouchPalette.IsFreezable="True" />
  <SolidColorBrush x:Key="InvertedForegroundBrush" Color="{Binding Path=Palette.InvertedForegroundColor, Source={StaticResource Windows8TouchColors}}" zinderud:Windows8TouchPalette.IsFreezable="True" />
  <zinderud:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
  <SolidColorBrush x:Key="OverlayBrush" Color="#FFFFFFFF" Opacity="0.6" />
  <SolidColorBrush x:Key="MainForegroundBrush" Color="{Binding Path=Palette.MainForegroundColor, Source={StaticResource Windows8TouchColors}}" zinderud:Windows8TouchPalette.IsFreezable="True" />
  <SolidColorBrush x:Key="LowBrush" Color="{Binding Path=Palette.LowColor, Source={StaticResource Windows8TouchColors}}" zinderud:Windows8TouchPalette.IsFreezable="True" />
  <SolidColorBrush x:Key="HighBrush" Color="{Binding Path=Palette.HighColor, Source={StaticResource Windows8TouchColors}}" zinderud:Windows8TouchPalette.IsFreezable="True" />
  <SolidColorBrush x:Key="AccentBrush" Color="{Binding Path=Palette.AccentColor, Source={StaticResource Windows8TouchColors}}" zinderud:Windows8TouchPalette.IsFreezable="True" />
  <SolidColorBrush x:Key="TileBackground" Color="#FF00A100" />
  <!-- Label-->
  <Style x:Key="LabelStyle" TargetType="zinderud:Label">
    <Setter Property="Background" Value="{x:Null}" />
    <Setter Property="BorderBrush" Value="{x:Null}" />
    <Setter Property="BorderThickness" Value="0" />
    <Setter Property="Foreground" Value="{zinderud:Windows8TouchResource ResourceKey=MainForegroundBrush}" />
    <Setter Property="FontFamily" Value="{zinderud:Windows8TouchResource ResourceKey=FontFamily}" />
    <Setter Property="FontSize" Value="{zinderud:Windows8TouchResource ResourceKey=FontSizeL}" />
    <Setter Property="HorizontalContentAlignment" Value="Stretch" />
    <Setter Property="VerticalContentAlignment" Value="Center" />
    <Setter Property="Padding" Value="5,3" />
    <Setter Property="IsTabStop" Value="False" />
    <Setter Property="FocusVisualStyle" Value="{x:Null}" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="zinderud:Label">
          <Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}">
            <VisualStateManager.VisualStateGroups>
              <VisualStateGroup x:Name="CommonStates">
                <VisualState x:Name="Normal" />
                <VisualState x:Name="Disabled">
                  <Storyboard>
                    <Storyboard>
                      <DoubleAnimation Duration="0" To="0.5" Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="Content" />
                    </Storyboard>
                  </Storyboard>
                </VisualState>
              </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid>
              <ContentPresenter x:Name="Content" Margin="{TemplateBinding Padding}" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" RecognizesAccessKey="True" TextBlock.Foreground="{TemplateBinding Foreground}" TextBlock.FontSize="{TemplateBinding FontSize}" TextBlock.FontFamily="{TemplateBinding FontFamily}" />
            </Grid>
          </Border>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
  <Style TargetType="zinderud:Label" BasedOn="{StaticResource LabelStyle}" />
  <!-- GroupBox-->
  <Style x:Key="GroupBoxStyle" TargetType="zinderud:GroupBox">
    <Setter Property="BorderBrush" Value="{zinderud:Windows8TouchResource ResourceKey=MediumBrush}" />
    <Setter Property="Foreground" Value="{zinderud:Windows8TouchResource ResourceKey=MainForegroundBrush}" />
    <Setter Property="FontFamily" Value="{zinderud:Windows8TouchResource ResourceKey=FontFamily}" />
    <Setter Property="FontSize" Value="{zinderud:Windows8TouchResource ResourceKey=FontSizeL}" />
    <Setter Property="BorderThickness" Value="1" />
    <Setter Property="Background" Value="{x:Null}" />
    <Setter Property="Header" Value="GroupBox" />
    <Setter Property="SnapsToDevicePixels" Value="True" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="zinderud:GroupBox">
          <Grid>
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="6" />
              <ColumnDefinition Width="Auto" />
              <ColumnDefinition Width="*" />
              <ColumnDefinition Width="6" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
              <RowDefinition Height="Auto" />
              <RowDefinition Height="Auto" />
              <RowDefinition Height="*" />
              <RowDefinition Height="6" />
            </Grid.RowDefinitions>
            <Border x:Name="Background" Grid.ColumnSpan="4" Grid.Column="0" Grid.Row="1" Grid.RowSpan="3" BorderBrush="{x:Null}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}" />
            <Border x:Name="PART_Header" Padding="5 0 5 1" Grid.Column="1" Grid.Row="0" Grid.RowSpan="2">
              <ContentPresenter ContentSource="Header" RecognizesAccessKey="True" />
            </Border>
            <ContentPresenter x:Name="Content" Grid.Row="2" Grid.ColumnSpan="2" Grid.Column="1" Margin="{TemplateBinding Padding}" />
            <Border x:Name="PART_Border" Margin="1 0 1 1" Grid.ColumnSpan="4" Grid.Row="1" Grid.RowSpan="3" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" IsHitTestVisible="False" />
          </Grid>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
  <Style TargetType="zinderud:GroupBox" BasedOn="{StaticResource GroupBoxStyle}" />
  <!-- RangeControl-->
  <Style x:Key="StepRepeatButtonBaseStyle" TargetType="RepeatButton">
    <Setter Property="Width" Value="22" />
    <Setter Property="Height" Value="22" />
    <Setter Property="Margin" Value="3" />
    <Setter Property="HorizontalAlignment" Value="Center" />
    <Setter Property="VerticalAlignment" Value="Center" />
  </Style>
  <Style x:Key="HorizontalIncreaseRepeatButtonStyle" TargetType="RepeatButton" BasedOn="{StaticResource StepRepeatButtonBaseStyle}">
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="RepeatButton">
          <Grid>
            <zinderudChromes:ButtonChrome RenderEnabled="{TemplateBinding IsEnabled}" RenderMouseOver="{TemplateBinding IsMouseOver}" RenderPressed="{TemplateBinding IsPressed}" />
            <ContentControl x:Name="Arrow" Template="{StaticResource ScrollArrow_Right}" Foreground="{StaticResource ButtonIconForeground_Normal}" Background="{StaticResource ButtonIconBackground_Normal}" />
          </Grid>
          <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
              <Setter TargetName="Arrow" Property="Foreground" Value="{StaticResource ButtonIconForeground_MouseOver}" />
              <Setter TargetName="Arrow" Property="Background" Value="{StaticResource ButtonIconBackground_MouseOver}" />
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
              <Setter TargetName="Arrow" Property="Foreground" Value="{StaticResource ButtonIconForeground_Pressed}" />
              <Setter TargetName="Arrow" Property="Background" Value="{StaticResource ButtonIconBackground_Pressed}" />
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
              <Setter TargetName="Arrow" Property="Foreground" Value="{StaticResource ButtonIconForeground_Disabled}" />
              <Setter TargetName="Arrow" Property="Background" Value="{StaticResource ButtonIconBackground_Disabled}" />
            </Trigger>
          </ControlTemplate.Triggers>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
  <Style x:Key="HorizontalDecreaseRepeatButtonStyle" TargetType="RepeatButton" BasedOn="{StaticResource StepRepeatButtonBaseStyle}">
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="RepeatButton">
          <Grid>
            <zinderudChromes:ButtonChrome RenderEnabled="{TemplateBinding IsEnabled}" RenderMouseOver="{TemplateBinding IsMouseOver}" RenderPressed="{TemplateBinding IsPressed}" />
            <ContentControl x:Name="Arrow" Template="{StaticResource ScrollArrow_Left}" Foreground="{StaticResource ButtonIconForeground_Normal}" Background="{StaticResource ButtonIconBackground_Normal}" />
          </Grid>
          <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
              <Setter TargetName="Arrow" Property="Foreground" Value="{StaticResource ButtonIconForeground_MouseOver}" />
              <Setter TargetName="Arrow" Property="Background" Value="{StaticResource ButtonIconBackground_MouseOver}" />
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
              <Setter TargetName="Arrow" Property="Foreground" Value="{StaticResource ButtonIconForeground_Pressed}" />
              <Setter TargetName="Arrow" Property="Background" Value="{StaticResource ButtonIconBackground_Pressed}" />
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
              <Setter TargetName="Arrow" Property="Foreground" Value="{StaticResource ButtonIconForeground_Disabled}" />
              <Setter TargetName="Arrow" Property="Background" Value="{StaticResource ButtonIconBackground_Disabled}" />
            </Trigger>
          </ControlTemplate.Triggers>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
  <Style x:Key="VerticalDecreaseRepeatButtonStyle" TargetType="RepeatButton" BasedOn="{StaticResource StepRepeatButtonBaseStyle}">
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="RepeatButton">
          <Grid>
            <zinderudChromes:ButtonChrome RenderEnabled="{TemplateBinding IsEnabled}" RenderMouseOver="{TemplateBinding IsMouseOver}" RenderPressed="{TemplateBinding IsPressed}" />
            <ContentControl x:Name="Arrow" Template="{StaticResource ScrollArrow_Down}" Foreground="{StaticResource ButtonIconForeground_Normal}" Background="{StaticResource ButtonIconBackground_Normal}" />
          </Grid>
          <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
              <Setter TargetName="Arrow" Property="Foreground" Value="{StaticResource ButtonIconForeground_MouseOver}" />
              <Setter TargetName="Arrow" Property="Background" Value="{StaticResource ButtonIconBackground_MouseOver}" />
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
              <Setter TargetName="Arrow" Property="Foreground" Value="{StaticResource ButtonIconForeground_Pressed}" />
              <Setter TargetName="Arrow" Property="Background" Value="{StaticResource ButtonIconBackground_Pressed}" />
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
              <Setter TargetName="Arrow" Property="Foreground" Value="{StaticResource ButtonIconForeground_Disabled}" />
              <Setter TargetName="Arrow" Property="Background" Value="{StaticResource ButtonIconBackground_Disabled}" />
