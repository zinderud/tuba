                        <Path x:Name="arrow" IsHitTestVisible="False" Fill="{zinderud:Windows8TouchResource ResourceKey=InvertedBrush}" HorizontalAlignment="Center" VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5" Stretch="None" Width="10" Height="8" Data="M 0 0 L 5 8 L 10 0 Z" SnapsToDevicePixels="True">
                          <Path.RenderTransform>
