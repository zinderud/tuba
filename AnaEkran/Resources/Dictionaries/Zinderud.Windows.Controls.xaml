  <Style TargetType="zinderud:SioFluidContentControl" BasedOn="{StaticResource RadFluidContentControlStyle}" />
  <!-- RadTileList-->
    <ControlTemplate x:Key="RadTileListTemplate" TargetType="zinderud:SioTileList">
    <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
      <StackPanel Orientation="Horizontal">
        <ContentControl x:Name="PART_PanoramaBackground" ClipToBounds="True" HorizontalAlignment="Left" VerticalAlignment="Top" Content="{TemplateBinding PanoramaBackground}" />
      </StackPanel>
      <ScrollViewer x:Name="PART_ScrollViewer" Margin="0" CanContentScroll="True" Padding="{TemplateBinding Padding}" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" HorizontalScrollBarVisibility="{TemplateBinding ScrollViewer.HorizontalScrollBarVisibility}" VerticalScrollBarVisibility="Hidden">
        <ItemsPresenter />
