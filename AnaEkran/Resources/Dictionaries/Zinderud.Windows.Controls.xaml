              <Rectangle x:Name="OldContentPresenter" Width="{Binding OldVisualWidth,            ElementName=ContentElement}" Height="{Binding OldVisualHeight,            ElementName=ContentElement}" Fill="{Binding OldVisualBrush,            ElementName=ContentElement}" Opacity="0.0" Canvas.ZIndex="1" transitionControl:LayoutAnimationPanel.HeightWeight="1" transitionControl:LayoutAnimationPanel.WidthWeight="1" />
            </transitionControl:LayoutAnimationPanel>
