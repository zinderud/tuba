                  <zinderud:SioButton IsEnabled="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsReadOnly, Converter={StaticResource InvertedBooleanConverter}}" Width="36" Height="36" Command="{x:Static propertyGrid:CollectionEditorCommands.MoveCurrentToPrevious}">
                    <Path Fill="Black" Stretch="Fill" Stroke="{x:Null}" Width="10" Height="8" RenderTransformOrigin="0.5,0.5" Data="M0,0 L7,0 L3.4999998,4 z" SnapsToDevicePixels="True">
                      <Path.RenderTransform>
                        <TransformGroup>
                          <RotateTransform Angle="-180" />
                        </TransformGroup>
                      </Path.RenderTransform>
                    </Path>
                  </zinderud:SioButton>
                </StackPanel>
              </Grid>
            </Border>
          </Border>
          <ListBox x:Name="PART_CollectionListBox" Grid.Row="1" ItemTemplate="{TemplateBinding ItemTemplate}" BorderBrush="{x:Null}" Background="Transparent" BorderThickness="0" ItemsSource="{TemplateBinding CollectionView}" SelectedItem="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=CurrentItem, Mode=TwoWay}" />
        </Grid>
        <Grid Grid.Row="1" Grid.Column="2">
          <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition />
          </Grid.RowDefinitions>
          <Border BorderBrush="{TemplateBinding BorderBrush}" MinHeight="48" BorderThickness="1,0,0,1">
            <TextBlock zinderud:LocalizationManager.ResourceKey="CollectionEditorProperties" Foreground="{StaticResource MainForegroundBrush}" Grid.Row="1" Grid.Column="2" FontSize="15" VerticalAlignment="Top" Padding="5,6,5,3" />
          </Border>
          <controls:SioPropertyGrid IsReadOnly="{TemplateBinding IsReadOnly}" Item="{TemplateBinding CurrentItem}" Background="Transparent" LabelColumnWidth="100" Grid.Row="1" BorderThickness="1,0,0,0" Margin="0,0,0,-1" SortAndGroupButtonsVisibility="Collapsed" SearchBoxVisibility="Collapsed" DescriptionPanelVisibility="Collapsed" />
        </Grid>
        <Border x:Name="FooterPanel_Background" Grid.Row="3" BorderBrush="{StaticResource MediumBrush}" BorderThickness="0,1,0,0" Grid.ColumnSpan="2" Background="{StaticResource MainBrush}">
          <Grid>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
              <zinderud:SioButton IsEnabled="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsReadOnly, Converter={StaticResource InvertedBooleanConverter}}" Width="105" Margin="5,6,5,7" zinderud:LocalizationManager.ResourceKey="CollectionEditorAdd" Command="{x:Static propertyGrid:CollectionEditorCommands.AddNew}" />
              <zinderud:SioButton IsEnabled="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsReadOnly, Converter={StaticResource InvertedBooleanConverter}}" Width="105" Margin="0,6,5,7" zinderud:LocalizationManager.ResourceKey="CollectionEditorRemove" Command="{x:Static propertyGrid:CollectionEditorCommands.Delete}" />
            </StackPanel>
            <Thumb x:Name="PART_ResizeThumb" Style="{StaticResource CollectionEditorGripperStyle}" Visibility="{TemplateBinding ResizeGripperVisibility}" />
          </Grid>
        </Border>
      </Grid>
    </Border>
  </ControlTemplate>
  <Style x:Key="CollectionEditorStyle" TargetType="propertyGrid:CollectionEditor">
    <Setter Property="Template" Value="{StaticResource CollectionEditor_Template}" />
    <Setter Property="Background" Value="{StaticResource MainBrush}" />
    <Setter Property="BorderBrush" Value="{StaticResource MediumBrush}" />
    <Setter Property="BorderThickness" Value="1" />
    <Setter Property="FontFamily" Value="Segoe UI" />
    <Setter Property="Width" Value="471" />
    <Setter Property="Height" Value="370" />
    <Setter Property="FontSize" Value="15" />
    <Setter Property="IsTabStop" Value="False" />
    <Setter Property="SnapsToDevicePixels" Value="True" />
  </Style>
  <Style TargetType="propertyGrid:CollectionEditor" BasedOn="{StaticResource CollectionEditorStyle}" />
  <!--CollectionEditorPicker-->
  <ControlTemplate x:Key="CollectionEditorPicker_Template" TargetType="propertyGrid:CollectionEditorPicker">
    <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
      <Grid>
        <Grid.ColumnDefinitions>
          <ColumnDefinition />
          <ColumnDefinition Width="Auto" />
        </Grid.ColumnDefinitions>
        <TextBlock Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Source}" VerticalAlignment="Center" Margin="4,0,0,0" x:Name="PART_CollectionEditorTextBlock" />
        <zinderud:SioDropDownButton IsEnabled="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsReadOnly, Converter={StaticResource InvertedBooleanConverter}}" Grid.Column="1" HorizontalContentAlignment="Left" Margin="0,-1,-1,-1" CornerRadius="0" Width="36" Padding="0" PopupPlacementTarget="{Binding RelativeSource={RelativeSource TemplatedParent}}">
          <zinderud:SioDropDownButton.DropDownContent>
            <propertyGrid:CollectionEditor x:Name="PART_CollectionEditor" Source="{Binding RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, Path=Source}" Header="{Binding RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, Path=Header}" ResizeGripperVisibility="Visible" Margin="-1" />
          </zinderud:SioDropDownButton.DropDownContent>
        </zinderud:SioDropDownButton>
      </Grid>
    </Border>
  </ControlTemplate>
  <Style x:Key="CollectionEditorPickerStyle" TargetType="propertyGrid:CollectionEditorPicker">
    <Setter Property="Template" Value="{StaticResource CollectionEditorPicker_Template}" />
    <Setter Property="Background" Value="{StaticResource MainBrush}" />
    <Setter Property="BorderBrush" Value="{StaticResource MediumBrush}" />
    <Setter Property="BorderThickness" Value="1" />
    <Setter Property="IsTabStop" Value="False" />
    <Setter Property="SnapsToDevicePixels" Value="True" />
  </Style>
  <Style TargetType="propertyGrid:CollectionEditorPicker" BasedOn="{StaticResource CollectionEditorPickerStyle}" />
  <!--FlagEnumEditor-->
  <ControlTemplate x:Key="FlagEnumEditorNonEditableTemplate" TargetType="propertyGrid:FlagEnumEditor">
    <zinderud:SioDropDownButton x:Name="PART_EnumValuesDropDownButton" HorizontalAlignment="Stretch" HorizontalContentAlignment="Stretch" IsTabStop="False" ClickMode="Press" Padding="0" TabIndex="1" VerticalAlignment="Stretch" VerticalContentAlignment="Center" Focusable="False">
      <zinderud:SioDropDownButton.DropDownContent>
        <ScrollViewer ScrollViewer.HorizontalScrollBarVisibility="Auto" ScrollViewer.VerticalScrollBarVisibility="Auto">
          <ItemsControl ItemsSource="{Binding EnumItemsSource, RelativeSource={RelativeSource TemplatedParent}}" BorderThickness="0">
            <ItemsControl.ItemTemplate>
              <DataTemplate>
                <CheckBox IsChecked="{Binding IsSelected, Mode=TwoWay}" Content="{Binding Value}" Margin="2,2,0,0" VerticalContentAlignment="Center" />
              </DataTemplate>
            </ItemsControl.ItemTemplate>
          </ItemsControl>
        </ScrollViewer>
      </zinderud:SioDropDownButton.DropDownContent>
