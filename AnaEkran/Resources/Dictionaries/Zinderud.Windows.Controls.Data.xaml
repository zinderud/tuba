      </ItemsControl.ItemsPanel>
      <ItemsControl.ItemTemplate>
        <DataTemplate>
          <zinderud:SioRadioButton IsChecked="{Binding IsCurrent, Mode=OneWay}" GroupName="{Binding GroupName}" Command="controls:SioDataPagerCommands.MoveToPage" CommandParameter="{Binding PageIndex}" Content="{Binding Content}" Style="{Binding NumericButtonStyle}" />
