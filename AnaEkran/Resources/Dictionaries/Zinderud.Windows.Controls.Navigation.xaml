ce:Zinderud.Windows.Controls.TileView;assembly=Zinderud.Windows.Controls.Navigation" xmlns:sys="clr-namespace:System;assembly=mscorlib" xmlns:local="clr-namespace:Zinderud.Windows.Controls;assembly=Zinderud.Windows.Controls.Navigation" xmlns:carousel="clr-namespace:Zinderud.Windows.Controls.Carousel;assembly=Zinderud.Windows.Controls.Navigation" xmlns:zinderudBreadcrumb="clr-namespace:Zinderud.Windows.Controls.Breadcrumb;assembly=Zinderud.Windows.Controls.Navigation">
