                  <Storyboard>
                    <ObjectAnimationUsingKeyFrames Storyboard.TargetName="DropImpossibleIconElement" Storyboard.TargetProperty="Visibility">
                      <DiscreteObjectKeyFrame KeyTime="0">
                        <DiscreteObjectKeyFrame.Value>
                          <Visibility>Visible</Visibility>
                        </DiscreteObjectKeyFrame.Value>
                      </DiscreteObjectKeyFrame>
                    </ObjectAnimationUsingKeyFrames>
                    <ObjectAnimationUsingKeyFrames Storyboard.TargetName="DropPossibleIconElement" Storyboard.TargetProperty="Visibility">
                      <DiscreteObjectKeyFrame KeyTime="0">
                        <DiscreteObjectKeyFrame.Value>
                          <Visibility>Collapsed</Visibility>
                        </DiscreteObjectKeyFrame.Value>
                      </DiscreteObjectKeyFrame>
                    </ObjectAnimationUsingKeyFrames>
                  </Storyboard>
                </VisualState>
              </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border BorderThickness="{TemplateBinding BorderThickness}" Grid.RowSpan="2" BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}"></Border>
            <Grid HorizontalAlignment="Left" Visibility="{TemplateBinding DragPreviewVisibility}">
              <ItemsPresenter Margin="{TemplateBinding Padding}" />
              <Grid x:Name="DraggedItemsCountContainer" Visibility="Collapsed" HorizontalAlignment="Center" VerticalAlignment="Center" Width="Auto" Height="Auto">
                <Rectangle Stroke="{zinderud:Windows8TouchResource ResourceKey=MediumBrush}" Fill="{TemplateBinding Background}" StrokeThickness="1" />
                <TextBlock x:Name="DraggedItemsCountText" Foreground="{zinderud:Windows8TouchResource ResourceKey=InvertedForegroundBrush}" HorizontalAlignment="{TemplateBinding HorizontalAlignment}" VerticalAlignment="{TemplateBinding VerticalAlignment}" Margin="8,3" Text="5" />
              </Grid>
            </Grid>
            <Border Background="{zinderud:Windows8TouchResource ResourceKey=AccentBrush}" Grid.Row="1" />
            <Grid Visibility="{TemplateBinding DragTooltipVisibility}" Grid.Row="1">
              <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
              </Grid.ColumnDefinitions>
