                <ContentControl VerticalAlignment="Center" Margin="0,0,15,0" Content="{TemplateBinding DragTooltipContent}" ContentTemplate="{TemplateBinding DragTooltipContentTemplate}" Foreground="{zinderud:Windows8TouchResource ResourceKey=InvertedForegroundBrush}" />
