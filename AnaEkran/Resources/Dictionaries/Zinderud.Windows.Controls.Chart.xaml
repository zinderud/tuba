  </Style>
  <Style TargetType="zinderudChartView:SplineSeries" BasedOn="{StaticResource SplineSeriesStyle}" />
  <Style x:Key="BarSeriesStyle" TargetType="zinderudChartView:BarSeries">
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="zinderudChartView:BarSeries">
          <Canvas x:Name="renderSurface" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
        </ControlTemplate>
      </Setter.Value>
    </Setter>
    <Setter Property="TrackBallTemplate" Value="{StaticResource TrackBallDataTemplate}" />
    <Setter Property="DefaultVisualStyle">
