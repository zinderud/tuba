      <Setter.Value>
        <ControlTemplate TargetType="zinderudChartView:AreaSeries">
          <Canvas x:Name="renderSurface" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
        </ControlTemplate>
      </Setter.Value>
    </Setter>
    <Setter Property="TrackBallTemplate" Value="{StaticResource TrackBallDataTemplate}" />
    <Setter Property="IsHitTestVisible" Value="False" />
    <Setter Property="StrokeShapeStyle">
      <Setter.Value>
        <Style TargetType="Path">
          <Setter Property="Stroke" Value="{StaticResource StrokeBrush}" />
          <Setter Property="StrokeThickness" Value="{StaticResource AreaStrokeThickness}" />
          <Setter Property="StrokeStartLineCap" Value="Triangle" />
          <Setter Property="StrokeEndLineCap" Value="Triangle" />
        </Style>
      </Setter.Value>
    </Setter>
    <Setter Property="AreaShapeStyle">
      <Setter.Value>
        <Style TargetType="Path">
          <Setter Property="Fill">
            <Setter.Value>
              <SolidColorBrush Color="{StaticResource AccentColor}" />
            </Setter.Value>
          </Setter>
        </Style>
      </Setter.Value>
    </Setter>
