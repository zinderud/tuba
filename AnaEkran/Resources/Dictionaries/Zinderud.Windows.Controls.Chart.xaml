<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:sys="clr-namespace:System;assembly=mscorlib" xmlns:zinderudControls="clr-namespace:Zinderud.Windows.Controls;assembly=Zinderud.Windows.Controls" xmlns:zinderudChart="clr-namespace:Zinderud.Windows.Controls;assembly=Zinderud.Windows.Controls.Chart" xmlns:zinderudChartView="clr-namespace:Zinderud.Windows.Controls.ChartView;assembly=Zinderud.Windows.Controls.Chart" xmlns:zinderudPrimitives="clr-namespace:Zinderud.Windows.Controls.Primitives;assembly=Zinderud.Windows.Controls">
  <zinderudControls:Windows8TouchColors x:Key="Windows8TouchColors" />
  <SolidColorBrush x:Key="ChartBackground" Color="#00FFFFFF" />
  <SolidColorBrush x:Key="AxisLabelForeground" Color="#FF333333" />
  <SolidColorBrush x:Key="AxisStrokeBrush" Color="#FF828282" />
  <DataTemplate x:Key="TrackBallDataTemplate">
    <Ellipse Height="8" Width="8" Fill="#FFFFFFFF" Stroke="#FF828282" StrokeThickness="1" />
  </DataTemplate>
  <SolidColorBrush x:Key="StrokeBrush" Color="#000000" />
  <sys:Double x:Key="AreaStrokeThickness">0</sys:Double>
  <Color x:Key="AccentColor">#FF5AA4D4</Color>
  <sys:Double x:Key="LineStrokeThickness">2</sys:Double>
  <Thickness x:Key="BarSeriesStrokeThickness">0</Thickness>
  <sys:Double x:Key="FinancialSeriesStrokeThickness">1</sys:Double>
  <SolidColorBrush x:Key="FinancialSeriesUpStroke" Color="#FFB2B2B2" />
  <SolidColorBrush x:Key="FinancialSeriesDownStroke" Color="#FF7A7A7A" />
  <SolidColorBrush x:Key="FinancialSeriesUpFill" Color="#FFD2D2D2" />
  <SolidColorBrush x:Key="FinancialSeriesDownFill" Color="#FF494949" />
  <SolidColorBrush x:Key="GridLineBrush" Color="#FFE4E4E4" />
  <ControlTemplate x:Key="HandleRepeatButtonTemplate" TargetType="RepeatButton">
    <Grid x:Name="Root" />
  </ControlTemplate>
  <Style x:Key="HandleStyle" TargetType="RepeatButton">
    <Setter Property="VerticalAlignment" Value="Stretch" />
    <Setter Property="Width" Value="0" />
    <Setter Property="Template" Value="{StaticResource HandleRepeatButtonTemplate}" />
    <Setter Property="BorderThickness" Value="0" />
    <Setter Property="BorderBrush" Value="Transparent" />
    <Setter Property="Background" Value="Transparent" />
  </Style>
  <Style x:Key="ThumbStyle" TargetType="Thumb">
    <Setter Property="Width" Value="24" />
    <Setter Property="VerticalAlignment" Value="Stretch" />
    <Setter Property="IsTabStop" Value="True" />
    <Setter Property="BorderThickness" Value="0" />
    <Setter Property="BorderBrush" Value="Transparent" />
    <Setter Property="Background" Value="{zinderudControls:Windows8TouchResource ResourceKey=InvertedBrush}" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="Thumb">
          <Grid>
            <VisualStateManager.VisualStateGroups>
              <VisualStateGroup x:Name="CommonStates">
                <VisualState x:Name="Normal" />
                <VisualState x:Name="MouseOver">
                  <Storyboard>
                    <DoubleAnimation Storyboard.TargetName="Border" Storyboard.TargetProperty="(Border.Opacity)" To="0.1" Duration="0:0:0" />
                  </Storyboard>
                </VisualState>
                <VisualState x:Name="Pressed">
                  <Storyboard>
                    <DoubleAnimation Storyboard.TargetName="Border" Storyboard.TargetProperty="(Border.Opacity)" To="1" Duration="0:0:0" />
                  </Storyboard>
                </VisualState>
              </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border x:Name="Border" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}" Opacity="0.2" />
          </Grid>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
  <Style x:Key="SelectionMiddleThumbStyle" TargetType="Thumb">
    <Setter Property="IsTabStop" Value="True" />
    <Setter Property="BorderThickness" Value="0" />
    <Setter Property="BorderBrush" Value="Transparent" />
    <Setter Property="Background" Value="{zinderudControls:Windows8TouchResource ResourceKey=InvertedBrush}" />
    <Setter Property="Cursor" Value="Hand" />
    <Setter Property="VerticalAlignment" Value="Stretch" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="Thumb">
          <Grid>
            <VisualStateManager.VisualStateGroups>
              <VisualStateGroup x:Name="CommonStates">
                <VisualState x:Name="Normal" />
                <VisualState x:Name="MouseOver">
                  <Storyboard>
                    <DoubleAnimation Storyboard.TargetName="Border" Storyboard.TargetProperty="(Border.Opacity)" To="0.1" Duration="0:0:0" />
                  </Storyboard>
                </VisualState>
                <VisualState x:Name="Pressed">
                  <Storyboard>
                    <DoubleAnimation Storyboard.TargetName="Border" Storyboard.TargetProperty="(Border.Opacity)" To="1" Duration="0:0:0" />
                  </Storyboard>
                </VisualState>
              </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border x:Name="Border" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}" Opacity="0.2" />
          </Grid>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
  <Style x:Key="TrackStyle" TargetType="ContentControl">
    <Setter Property="IsTabStop" Value="False" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="ContentControl">
          <Border BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}" />
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
  <Style x:Key="LargeHandleStyle" TargetType="RepeatButton">
    <Setter Property="IsTabStop" Value="False" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="RepeatButton">
          <Grid Background="Transparent" />
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
  <Style x:Key="PanZoomBarStyle" TargetType="zinderudChartView:PanZoomBar">
    <Setter Property="HandlesVisibility" Value="Collapsed" />
    <Setter Property="IsMouseWheelEnabled" Value="False" />
    <Setter Property="IsSelectionRangeEnabled" Value="True" />
    <Setter Property="StepAction" Value="MoveRange" />
    <Setter Property="BorderBrush" Value="Transparent" />
    <Setter Property="BorderThickness" Value="0" />
    <Setter Property="ThumbStyle" Value="{StaticResource ThumbStyle}" />
    <Setter Property="SelectionMiddleThumbStyle" Value="{StaticResource SelectionMiddleThumbStyle}" />
    <Setter Property="TrackStyle" Value="{StaticResource TrackStyle}" />
    <Setter Property="IncreaseHandleStyle" Value="{StaticResource HandleStyle}" />
    <Setter Property="DecreaseHandleStyle" Value="{StaticResource HandleStyle}" />
    <Setter Property="IsTabStop" Value="False" />
    <Setter Property="SmallChange" Value="0.01" />
    <Setter Property="LargeChange" Value="0.1" />
    <Setter Property="Minimum" Value="0" />
    <Setter Property="Maximum" Value="1" />
    <Setter Property="MinimumRangeSpan" Value="0.01" />
    <Setter Property="Padding" Value="0 6 0 2" />
    <Setter Property="Height" Value="24" />
    <Setter Property="Width" Value="24" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="zinderudChartView:PanZoomBar">
          <zinderudPrimitives:LayoutTransformControl x:Name="layoutControl" RenderTransformOrigin=".5 0">
            <zinderudPrimitives:LayoutTransformControl.LayoutTransform>
              <TransformGroup>
                <ScaleTransform />
                <SkewTransform />
                <RotateTransform />
                <TranslateTransform />
              </TransformGroup>
            </zinderudPrimitives:LayoutTransformControl.LayoutTransform>
            <VisualStateManager.VisualStateGroups>
              <VisualStateGroup x:Name="OrientationStates">
                <VisualState x:Name="Horizontal">
                  <Storyboard>
                    <ObjectAnimationUsingKeyFrames Storyboard.TargetName="layoutControl" Storyboard.TargetProperty="LayoutTransform">
                      <DiscreteObjectKeyFrame KeyTime="0">
                        <DiscreteObjectKeyFrame.Value>
                          <RotateTransform Angle="0" />
                        </DiscreteObjectKeyFrame.Value>
                      </DiscreteObjectKeyFrame>
                    </ObjectAnimationUsingKeyFrames>
                  </Storyboard>
                </VisualState>
                <VisualState x:Name="Vertical">
                  <Storyboard>
                    <ObjectAnimationUsingKeyFrames Storyboard.TargetName="layoutControl" Storyboard.TargetProperty="LayoutTransform">
                      <DiscreteObjectKeyFrame KeyTime="0">
                        <DiscreteObjectKeyFrame.Value>
                          <RotateTransform Angle="-90" />
                        </DiscreteObjectKeyFrame.Value>
                      </DiscreteObjectKeyFrame>
                    </ObjectAnimationUsingKeyFrames>
                    <ObjectAnimationUsingKeyFrames Storyboard.TargetName="RangeStartThumb" Storyboard.TargetProperty="(FrameworkElement.Cursor)">
                      <DiscreteObjectKeyFrame KeyTime="0">
                        <DiscreteObjectKeyFrame.Value>
                          <Cursor>SizeNS</Cursor>
                        </DiscreteObjectKeyFrame.Value>
                      </DiscreteObjectKeyFrame>
                    </ObjectAnimationUsingKeyFrames>
                    <ObjectAnimationUsingKeyFrames Storyboard.TargetName="RangeEndThumb" Storyboard.TargetProperty="(FrameworkElement.Cursor)">
                      <DiscreteObjectKeyFrame KeyTime="0">
                        <DiscreteObjectKeyFrame.Value>
                          <Cursor>SizeNS</Cursor>
                        </DiscreteObjectKeyFrame.Value>
                      </DiscreteObjectKeyFrame>
                    </ObjectAnimationUsingKeyFrames>
                  </Storyboard>
                </VisualState>
              </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid>
              <Border BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}" />
              <Grid>
                <Grid.ColumnDefinitions>
                  <ColumnDefinition Width="Auto" />
                  <ColumnDefinition Width="Auto" />
                  <ColumnDefinition Width="Auto" />
                  <ColumnDefinition Width="*" />
                  <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>
                <ContentControl Grid.Column="1" Grid.ColumnSpan="3" Style="{TemplateBinding TrackStyle}" x:Name="Track" />
                <RepeatButton x:Name="DecreaseButton" Delay="{TemplateBinding Delay}" Interval="{TemplateBinding RepeatInterval}" Visibility="{TemplateBinding HandlesVisibility}" Style="{TemplateBinding DecreaseHandleStyle}" />
                <RepeatButton x:Name="LargeDecreaseButton" Grid.Column="1" Style="{StaticResource LargeHandleStyle}" Delay="{TemplateBinding Delay}" Interval="{TemplateBinding RepeatInterval}" />
                <Grid Grid.Column="2" Margin="{TemplateBinding Padding}">
                  <Thumb x:Name="SingleThumbHost" Style="{TemplateBinding ThumbStyle}" Visibility="Collapsed" />
                  <StackPanel x:Name="RangeThumbsPanel" Orientation="Horizontal">
                    <Thumb x:Name="RangeStartThumb" Style="{TemplateBinding ThumbStyle}" Cursor="SizeWE" Visibility="{TemplateBinding ThumbVisibility}" />
                    <Thumb x:Name="RangeMiddleThumb" Style="{TemplateBinding SelectionMiddleThumbStyle}" />
                    <Thumb x:Name="RangeEndThumb" Style="{TemplateBinding ThumbStyle}" Cursor="SizeWE" Visibility="{TemplateBinding ThumbVisibility}" />
                  </StackPanel>
                </Grid>
                <RepeatButton x:Name="LargeIncreaseButton" Grid.Column="3" Style="{StaticResource LargeHandleStyle}" Delay="{TemplateBinding Delay}" Interval="{TemplateBinding RepeatInterval}" />
                <RepeatButton x:Name="IncreaseButton" Grid.Row="1" Grid.Column="4" Delay="{TemplateBinding Delay}" Interval="{TemplateBinding RepeatInterval}" Visibility="{TemplateBinding HandlesVisibility}" RenderTransformOrigin="0.5,0.5" Style="{TemplateBinding IncreaseHandleStyle}" />
              </Grid>
            </Grid>
          </zinderudPrimitives:LayoutTransformControl>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
  <Style TargetType="zinderudChartView:PanZoomBar" BasedOn="{StaticResource PanZoomBarStyle}" />
  <Style x:Key="SioChartBaseStyle" TargetType="zinderudChartView:SioChartBase">
    <Setter Property="FontFamily" Value="{zinderudControls:Windows8TouchResource ResourceKey=FontFamily}" />
    <Setter Property="FontSize" Value="{zinderudControls:Windows8TouchResource ResourceKey=FontSize}" />
    <Setter Property="Foreground" Value="{zinderudControls:Windows8TouchResource ResourceKey=MainForegroundBrush}" />
    <Setter Property="MinWidth" Value="100" />
    <Setter Property="MinHeight" Value="100" />
    <Setter Property="Background" Value="{StaticResource ChartBackground}" />
    <Setter Property="HorizontalContentAlignment" Value="Center" />
    <Setter Property="VerticalContentAlignment" Value="Center" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="zinderudChartView:SioChartBase">
          <Border x:Name="layoutRoot" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
            <Grid>
              <ContentPresenter x:Name="emptyContent" IsHitTestVisible="False" Content="{TemplateBinding EmptyContent}" ContentTemplate="{TemplateBinding EmptyContentTemplate}" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" Visibility="Collapsed" />
              <Canvas x:Name="adornerContainer" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Background="Transparent">
                <Canvas x:Name="labelContainer" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                  <Canvas x:Name="renderSurface" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                    <Border x:Name="plotAreaDecoration" Style="{TemplateBinding PlotAreaStyle}" />
                  </Canvas>
                </Canvas>
              </Canvas>
            </Grid>
          </Border>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
    <Setter Property="TrackBallLineStyle">
      <Setter.Value>
        <Style TargetType="Polyline">
          <Setter Property="Stroke" Value="#FF828282" />
          <Setter Property="StrokeThickness" Value="2" />
        </Style>
      </Setter.Value>
    </Setter>
    <Setter Property="DragZoomBorderStyle">
      <Setter.Value>
        <Style TargetType="Border">
          <Setter Property="BorderBrush" Value="#FF828282" />
          <Setter Property="BorderThickness" Value="1" />
          <Setter Property="Background" Value="#40FFFFFF" />
        </Style>
      </Setter.Value>
    </Setter>
  </Style>
  <Style TargetType="zinderudChart:SioCartesianChart" BasedOn="{StaticResource SioChartBaseStyle}" />
  <Style TargetType="zinderudChart:SioPolarChart" BasedOn="{StaticResource SioChartBaseStyle}" />
  <Style TargetType="zinderudChart:SioPieChart" BasedOn="{StaticResource SioChartBaseStyle}" />
  <Style x:Key="ChartElementPresenterStyle" TargetType="zinderudChartView:ChartElementPresenter">
    <Setter Property="HorizontalAlignment" Value="Stretch" />
    <Setter Property="VerticalAlignment" Value="Stretch" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="zinderudChartView:ChartElementPresenter">
          <Canvas x:Name="renderSurface" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
  <Style TargetType="zinderudChartView:ChartElementPresenter" BasedOn="{StaticResource ChartElementPresenterStyle}" />
  <Style x:Key="TrackBallInfoControlStyle" TargetType="zinderudChartView:TrackBallInfoControl">
    <Setter Property="FontFamily" Value="{zinderudControls:Windows8TouchResource ResourceKey=FontFamily}" />
    <Setter Property="FontSize" Value="{zinderudControls:Windows8TouchResource ResourceKey=FontSize}" />
    <Setter Property="Foreground" Value="#FF000000" />
    <Setter Property="Background" Value="#FFFAFAFA" />
    <Setter Property="BorderBrush" Value="#FF828282" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="zinderudChartView:TrackBallInfoControl">
          <Grid>
            <Rectangle Margin="0 2" RadiusX="4" RadiusY="4" Stroke="{TemplateBinding BorderBrush}" Fill="{TemplateBinding Background}" SnapsToDevicePixels="True" />
            <Path Data="M0,1 C0,3 2,5 4,5 C4,5 4,8 4,8 C2,8 0,6 0,4 C0,4 0,2 0,1 z" Fill="{TemplateBinding BorderBrush}" HorizontalAlignment="Left" VerticalAlignment="Bottom" Height="7" Width="4" Stretch="Fill" SnapsToDevicePixels="True" />
            <Path Data="M0,1 C0,3 2,5 4,5 C4,5 4,8 4,8 C2,8 0,6 0,4 C0,4 0,2 0,1 z" Fill="{TemplateBinding BorderBrush}" Stretch="Fill" Height="7" Width="4" HorizontalAlignment="Right" VerticalAlignment="Bottom" RenderTransformOrigin="0.5,0.5" SnapsToDevicePixels="True">
              <Path.RenderTransform>
                <ScaleTransform ScaleX="-1" />
              </Path.RenderTransform>
            </Path>
            <Path Data="M0,1 C0,3 2,5 4,5 C4,5 4,8 4,8 C2,8 0,6 0,4 C0,4 0,2 0,1 z" Fill="{TemplateBinding BorderBrush}" HorizontalAlignment="Left" VerticalAlignment="Top" Height="7" Width="4" Stretch="Fill" RenderTransformOrigin="0.5,0.5" SnapsToDevicePixels="True">
              <Path.RenderTransform>
                <ScaleTransform ScaleY="-1" />
              </Path.RenderTransform>
            </Path>
            <Path Data="M0,1 C0,3 2,5 4,5 C4,5 4,8 4,8 C2,8 0,6 0,4 C0,4 0,2 0,1 z" Fill="{TemplateBinding BorderBrush}" HorizontalAlignment="Right" VerticalAlignment="Top" Width="4" Height="7" Stretch="Fill" RenderTransformOrigin="0.5,0.5" SnapsToDevicePixels="True">
              <Path.RenderTransform>
                <ScaleTransform ScaleY="-1" ScaleX="-1" />
              </Path.RenderTransform>
            </Path>
            <Rectangle Fill="{TemplateBinding BorderBrush}" HorizontalAlignment="Stretch" VerticalAlignment="Top" Height="3" Margin="3.8 0" SnapsToDevicePixels="True" />
            <Rectangle Fill="{TemplateBinding BorderBrush}" HorizontalAlignment="Stretch" VerticalAlignment="Bottom" Height="3" Margin="3.8 0" SnapsToDevicePixels="True" />
            <Grid Margin="3">
              <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="*" />
              </Grid.RowDefinitions>
              <ContentControl Name="header" Content="{TemplateBinding Header}" ContentTemplate="{TemplateBinding HeaderTemplate}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="4 0 4 4" FontWeight="Bold" />
              <StackPanel Name="panel" Grid.Row="1" Margin="0 0 0 4" />
            </Grid>
          </Grid>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
    <Setter Property="DataPointInfoTemplate">
      <Setter.Value>
        <DataTemplate>
          <Grid>
            <!--<Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <TextBlock Text="{Binding Path=DisplayHeader}" FontSize="12" Margin="4 2 4 0"/>-->
            <TextBlock Text="{Binding Path=DisplayContent}" Margin="4 0 4 4" />
          </Grid>
        </DataTemplate>
      </Setter.Value>
    </Setter>
  </Style>
  <Style TargetType="zinderudChartView:TrackBallInfoControl" BasedOn="{StaticResource TrackBallInfoControlStyle}" />
  <!--Axes-->
  <Style x:Key="AxisStyle" TargetType="zinderudChartView:Axis">
    <Setter Property="HorizontalAlignment" Value="Stretch" />
    <Setter Property="VerticalAlignment" Value="Stretch" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="zinderudChartView:Axis">
          <Canvas x:Name="renderSurface" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
        </ControlTemplate>
      </Setter.Value>
    </Setter>
    <Setter Property="LabelStyle">
      <Setter.Value>
        <Style TargetType="TextBlock">
          <Setter Property="Margin" Value="4" />
          <Setter Property="Foreground" Value="{StaticResource AxisLabelForeground}" />
        </Style>
      </Setter.Value>
    </Setter>
    <Setter Property="MajorTickStyle">
      <Setter.Value>
        <Style TargetType="Rectangle">
          <Setter Property="Fill" Value="{StaticResource AxisStrokeBrush}" />
        </Style>
      </Setter.Value>
    </Setter>
    <Setter Property="TitleTemplate">
      <Setter.Value>
        <DataTemplate>
          <TextBlock Text="{Binding}" />
        </DataTemplate>
      </Setter.Value>
    </Setter>
    <Setter Property="LineStroke" Value="{StaticResource AxisStrokeBrush}" />
  </Style>
  <Style TargetType="zinderudChartView:CategoricalAxis" BasedOn="{StaticResource AxisStyle}" />
  <Style TargetType="zinderudChartView:DateTimeCategoricalAxis" BasedOn="{StaticResource AxisStyle}" />
  <Style TargetType="zinderudChartView:DateTimeContinuousAxis" BasedOn="{StaticResource AxisStyle}" />
  <Style TargetType="zinderudChartView:LinearAxis" BasedOn="{StaticResource AxisStyle}" />
  <Style TargetType="zinderudChartView:PolarAxis" BasedOn="{StaticResource AxisStyle}" />
  <Style x:Key="LogarithmicAxisStyle" TargetType="zinderudChartView:LogarithmicAxis">
    <Setter Property="HorizontalAlignment" Value="Stretch" />
    <Setter Property="VerticalAlignment" Value="Stretch" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="zinderudChartView:LogarithmicAxis">
          <Canvas x:Name="renderSurface" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
        </ControlTemplate>
      </Setter.Value>
    </Setter>
    <Setter Property="LabelStyle">
      <Setter.Value>
        <Style TargetType="TextBlock">
          <Setter Property="Margin" Value="4" />
          <Setter Property="Foreground" Value="{StaticResource AxisLabelForeground}" />
        </Style>
      </Setter.Value>
    </Setter>
    <Setter Property="MajorTickStyle">
      <Setter.Value>
        <Style TargetType="Rectangle">
          <Setter Property="Fill" Value="{StaticResource AxisStrokeBrush}" />
        </Style>
      </Setter.Value>
    </Setter>
    <Setter Property="TitleTemplate">
      <Setter.Value>
        <DataTemplate>
          <TextBlock Text="{Binding}" />
        </DataTemplate>
      </Setter.Value>
    </Setter>
    <Setter Property="LineStroke" Value="{StaticResource AxisStrokeBrush}" />
    <Setter Property="RangeExtendDirection" Value="None" />
  </Style>
  <Style TargetType="zinderudChartView:LogarithmicAxis" BasedOn="{StaticResource LogarithmicAxisStyle}" />
  <Style x:Key="CategoricalRadialAxisStyle" TargetType="zinderudChartView:CategoricalRadialAxis" BasedOn="{StaticResource AxisStyle}">
    <Setter Property="LabelStyle">
      <Setter.Value>
        <Style TargetType="TextBlock">
          <Setter Property="Margin" Value="14" />
          <Setter Property="Foreground" Value="{StaticResource AxisLabelForeground}" />
        </Style>
      </Setter.Value>
    </Setter>
  </Style>
  <Style TargetType="zinderudChartView:CategoricalRadialAxis" BasedOn="{StaticResource CategoricalRadialAxisStyle}" />
  <Style x:Key="NumericRadialAxisStyle" TargetType="zinderudChartView:NumericRadialAxis" BasedOn="{StaticResource AxisStyle}">
    <Setter Property="LabelStyle">
      <Setter.Value>
        <Style TargetType="TextBlock">
          <Setter Property="Margin" Value="10" />
          <Setter Property="Foreground" Value="{StaticResource AxisLabelForeground}" />
        </Style>
      </Setter.Value>
    </Setter>
  </Style>
  <Style TargetType="zinderudChartView:NumericRadialAxis" BasedOn="{StaticResource NumericRadialAxisStyle}" />
  <!--Categorical Series-->
