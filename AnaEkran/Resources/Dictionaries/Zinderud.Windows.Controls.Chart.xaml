  <Style TargetType="zinderudChartView:AreaSeries" BasedOn="{StaticResource AreaSeriesStyle}" />
  <Style TargetType="zinderudChartView:SplineAreaSeries" BasedOn="{StaticResource AreaSeriesStyle}" />
  <Style x:Key="LineSeriesStyle" TargetType="zinderudChartView:LineSeries">
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="zinderudChartView:LineSeries">
          <Canvas x:Name="renderSurface" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
        </ControlTemplate>
      </Setter.Value>
    </Setter>
    <Setter Property="TrackBallTemplate" Value="{StaticResource TrackBallDataTemplate}" />
    <Setter Property="IsHitTestVisible" Value="False" />
    <Setter Property="StrokeShapeStyle">
      <Setter.Value>
        <Style TargetType="Path">
          <Setter Property="Stroke">
            <Setter.Value>
              <SolidColorBrush Color="{StaticResource AccentColor}" />
            </Setter.Value>
          </Setter>
          <Setter Property="StrokeThickness" Value="{StaticResource LineStrokeThickness}" />
        </Style>
      </Setter.Value>
    </Setter>
  </Style>
  <Style TargetType="zinderudChartView:LineSeries" BasedOn="{StaticResource LineSeriesStyle}" />
  <Style x:Key="SplineSeriesStyle" TargetType="zinderudChartView:SplineSeries">
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="zinderudChartView:SplineSeries">
          <Canvas x:Name="renderSurface" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
        </ControlTemplate>
      </Setter.Value>
    </Setter>
    <Setter Property="TrackBallTemplate" Value="{StaticResource TrackBallDataTemplate}" />
    <Setter Property="IsHitTestVisible" Value="False" />
    <Setter Property="StrokeShapeStyle">
      <Setter.Value>
        <Style TargetType="Path">
          <Setter Property="Stroke">
