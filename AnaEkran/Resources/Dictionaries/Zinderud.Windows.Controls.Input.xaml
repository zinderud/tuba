          <zinderud:SioListBox x:Name="PART_ListBox" ItemsSource="{TemplateBinding FilteredItems}" ItemTemplate="{TemplateBinding DropDownItemTemplate}" Margin="0 1 0 0" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
        </Grid>
      </Popup>
    </Grid>
  </ControlTemplate>
  <Style x:Key="SioAutoCompleteBoxStyle" TargetType="zinderudInput:SioAutoCompleteBox">
    <Setter Property="IsTabStop" Value="False" />
    <Setter Property="TextBoxStyle" Value="{StaticResource SioAutoCompleteTextBoxStyle}" />
    <Setter Property="Template" Value="{StaticResource SioAutoCompleteBoxTemplate}" />
    <Setter Property="BorderBrush" Value="{zinderud:Windows8TouchResource ResourceKey=MediumBrush}" />
    <Setter Property="BorderThickness" Value="1" />
    <Setter Property="Background" Value="{zinderud:Windows8TouchResource ResourceKey=MainBrush}" />
    <Setter Property="Padding" Value="0 0 3 3" />
    <Setter Property="Cursor" Value="IBeam" />
    <Setter Property="FontSize" Value="{zinderud:Windows8TouchResource ResourceKey=FontSizeL}" />
    <Setter Property="FontFamily" Value="{zinderud:Windows8TouchResource ResourceKey=FontFamily}" />
    <Setter Property="SnapsToDevicePixels" Value="True" />
  </Style>
  <Style TargetType="zinderudInput:SioAutoCompleteBox" BasedOn="{StaticResource SioAutoCompleteBoxStyle}" />
