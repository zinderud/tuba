            <Border x:Name="hover" BorderThickness="{TemplateBinding BorderThickness}" Background="Transparent" BorderBrush="{zinderud:Windows8TouchResource ResourceKey=InvertedBrush}" Opacity="0" IsHitTestVisible="False" SnapsToDevicePixels="True" />
